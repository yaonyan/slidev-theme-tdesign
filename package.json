{
  "name": "slidev-theme-tdesign",
  "version": "0.0.0",
  "type": "module",
  "keywords": [
    "slidev-theme",
    "slidev"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "slidev build example.md",
    "dev": "slidev example.md",
    "export": "slidev export example.md",
    "screenshot": "slidev export example.md --format png"
  },
  "dependencies": {
    "@slidev/types": "^51.8.2",
    "tdesign-icons-vue-next": "^0.3.6",
    "tdesign-vue-next": "^1.13.2"
  },
  "devDependencies": {
    "@slidev/cli": "^51.8.2"
  },
  "//": "Learn more: https://sli.dev/guide/write-theme.html",
  "slidev": {
    "colorSchema": "both",
    "defaults": {
      "fonts": {
        "sans": "Nunito Sans",
        "mono": "Fira Code"
      }
    }
  },
  "volta": {
    "node": "22.16.0"
  }
}
